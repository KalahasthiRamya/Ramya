#include<stdio.h>

int fib(int n);

int main(){
    int n = 10,i,sum = 0;
    for(i = 0;i<n;i++){
        printf("%d , ",fib(i));
        sum += fib(i);
    }
    printf("\nsum = %d",sum);
    return 0;
}

int fib(int n){
    if(n == 0)
    return 0;
    else if(n == 1)
    return 1;
    else
    return fib(n-1) + fib(n-2);
}
