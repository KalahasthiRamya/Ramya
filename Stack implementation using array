#include<stdio.h>
#include<stdlib.h>
#define size 5
int top = -1,arr[size];
void push();
void pop();
void display();

int main(){
    int i;
    for(i = 1;i<=size;i++){
        push();
    }
    pop();
    pop();
    display();
    return 0;
}

void push(){
    int x;
    if(top == size-1){
        printf("over flow\n");
    }
    else{
        printf("enter the data : ");
        scanf("%d",&x);
        top = top + 1;
        arr[top] = x;
    }
}

void pop(){
    if(top == -1){
        printf("under flow\n");
    }
    else{
        top = top - 1;
    }
}

void display(){
    int i;
    printf("\n\nstack : \n");
    for(i=top;i>=0;i--){
        printf("%d\n",arr[i]);
    }
}
